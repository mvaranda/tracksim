{
    "version": "0.2.0",
    "configurations": [
     {
            "name": "tracksim",
            "type": "cppdbg",
            // "preLaunchTask": "",
            "request": "launch",
            "program": "${workspaceRoot}/build/Desktop_Qt_6_7_2-Debug/tracksim",
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/",
            //"environment": [{"GTK_THEME": "light", "PYTHONPATH": "$PWD" }],
            "miDebuggerArgs": "",
