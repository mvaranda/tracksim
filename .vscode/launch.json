            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "environment": [
                // {
                //     "GTK_THEME": "light",
                //     "PYTHONPATH": "$PWD"
                // }
            ]
        }

    ]
